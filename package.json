{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "Showcase of a few projects over the past years. Portfolio of Deanna Bosschert, frontend web developer.",
  "scripts": {
    "predev": "rimraf _site",
    "dev:css": "sass --watch src/assets/scss:_site/assets/",
    "dev:eleventy": "npx @11ty/eleventy --formats=html,gif,jpg,jpeg,png,css --serve --port=3000",
    "dev": "cross-env ELEVENTY_ENV=development run-p dev:*",
    "debug": "DEBUG=* eleventy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deannabosschert/portfolio.git"
  },
  "keywords": [],
  "author": "Deanna Bosschert",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/deannabosschert/portfolio/issues"
  },
  "homepage": "https://github.com/deannabosschert/portfolio#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "cross-env": "^7.0.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  }
}
